  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wiltrovira/nsp-nodesecurity-calificaciones-201/issues"
  },
  "homepage": "https://github.com/wiltrovira/nsp-nodesecurity-calificaciones-201#readme",
  "dependencies": {
    "express": "^4.15.4"
  },
  "devDependencies": {
    "localtunnel": "^1.8.3",
    "nodeunit": "^0.11.1",
    "npm-run-all": "^4.0.2",
